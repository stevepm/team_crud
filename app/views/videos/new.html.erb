<h1>Add a Video</h1>

<%= form_for @video do |f| %>

  <p>
    <%= f.label :url %>
    <%= f.text_field :url %>
  </p>

  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>

  <p>
    <%= f.label :rating %>
    <%= f.text_field :rating %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
